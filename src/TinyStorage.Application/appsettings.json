{
  "ApplicationBasePath": "http://localhost:8080/",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenApiOAuthFlow": {
    "AuthorizationUrl": "http://localhost:5050/realms/tiny-storage-realm/protocol/openid-connect/auth",
    "TokenUrl": "http://localhost:5050/realms/tiny-storage-realm/protocol/openid-connect/token",
    "Scopes": {
      "openid": "Open ID",
      "profile": "Profile"
    }
  },
  "JwtAuthSettings": {
    "Authority": "http://localhost:5050/realms/tiny-storage-realm",
    "RequireHttpsMetadata": false,
    "TokenValidationParameters": {
      "RoleClaimType": "tags",
      "ValidIssuers": [
        "http://localhost:5050/realms/tiny-storage-realm"
      ],
      "ValidateAudience": false
    }
  },
  "AllowedHosts": "*"
}
